<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <Title>Mosa.Tool.Compiler</Title>
    <Company>MOSA Project</Company>
    <Copyright>Copyright © 2019</Copyright>
    <LangVersion>8.0</LangVersion>
    <DebugType>full</DebugType>
    <OutputPath>..\..\bin\</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>True</DebugSymbols>
    <WarningLevel>2</WarningLevel>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject>Mosa.Tool.Mosactl.Program</StartupObject>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\Mosa.Utility.Launcher\AppLocations.cs" Link="AppLocations.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Help\usage.txt" />
  </ItemGroup>
  <ItemGroup>
    <None Include="readme.md" />
  </ItemGroup>
</Project>
