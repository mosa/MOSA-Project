<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <AssemblyTitle>Mosa.Tool.Compiler</AssemblyTitle>
    <Company>MOSA Project</Company>
    <Product>Mosa.Tool.Compiler</Product>
    <Copyright>Copyright © 2019</Copyright>
    <ConfigurationOverrideFile />
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <FileAlignment>4096</FileAlignment>
    <RemoveIntegerChecks>False</RemoveIntegerChecks>
    <RegisterForComInterop>False</RegisterForComInterop>
    <BaseAddress>285212672</BaseAddress>
    <DebugType>full</DebugType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <OutputPath>..\..\bin\</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>True</DebugSymbols>
    <WarningLevel>2</WarningLevel>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject>Mosa.Tool.Mosactl.Program</StartupObject>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\Mosa.Utility.Launcher\AppLocations.cs" Link="AppLocations.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Help\usage.txt" />
  </ItemGroup>
  <ItemGroup>
    <None Include="readme.md" />
  </ItemGroup>
</Project>
