<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<OutputType>Exe</OutputType>
		<AssemblyTitle>Mosa.Utility.SourceCodeGenerator</AssemblyTitle>
		<Company>MOSA Project</Company>
		<Product>Mosa.Utility.SourceCodeGenerator</Product>
		<copyright>Copyright © 2022</copyright>
		<DebugType>full</DebugType>
		<LangVersion>9.0</LangVersion>
		<OutputPath>..\..\bin\</OutputPath>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<NoWarn>1591</NoWarn>
		<DebugSymbols>True</DebugSymbols>
		<UseAppHost>False</UseAppHost>
	</PropertyGroup>
	<PropertyGroup>
		<StartupObject>Mosa.Utility.SourceCodeGenerator.Program</StartupObject>
	</PropertyGroup>
	<ItemGroup>
		<ProjectReference Include="..\Mosa.Compiler.Common\Mosa.Compiler.Common.csproj" />
	</ItemGroup>
</Project>