<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <AssemblyName>Mosa.TestWorld.x64</AssemblyName>
    <OutputType>Exe</OutputType>
    <RootNamespace>Mosa.TestWorld.x64</RootNamespace>
    <StartupObject>Mosa.TestWorld.x64.Boot</StartupObject>
    <DebugType>full</DebugType>
    <LangVersion>8.0</LangVersion>
    <OutputPath>..\..\bin\</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DisableStandardFrameworkResolution>true</DisableStandardFrameworkResolution>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Mosa.Kernel.x64\Mosa.Kernel.x64.csproj" />
    <ProjectReference Include="..\Mosa.Korlib\Mosa.Korlib.csproj" />
    <ProjectReference Include="..\Mosa.Runtime.x64\Mosa.Runtime.x64.csproj" />
    <ProjectReference Include="..\Mosa.Runtime\Mosa.Runtime.csproj" />
    <ProjectReference Include="..\Mosa.UnitTests\Mosa.UnitTests.csproj" />
  </ItemGroup>
</Project>