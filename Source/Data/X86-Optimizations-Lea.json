{
	"Family": "x86",
	"Section": "Optimizations.Auto",
	"Commutative": [
		"X86.Add32",
		"X86.And32",
		"X86.Or32",
		"X86.Xor32"
	],
	"Optimizations": [
		{
			"Type": "Lea",
			"Name": "Add32",
			"SubName": "ToLea32Constant",
			"Expression": "IR.Add32 a b",
			"Filter": "IsResolvedConstant(b)",
			"Result": "(X86.Lea32 a 0 1 b)",
			"Priority": "45",
			"Variations": "Yes",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Add32",
			"SubName": "ToLea32",
			"Expression": "IR.Add32 a b",
			"Filter": "!IsConstant(b)",
			"Result": "(X86.Lea32 a b 1 0)",
			"Priority": "45",
			"Variations": "Yes",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Add32ShiftLeft3232",
			"SubName": "ToLea32",
			"Expression": "IR.Add32 a (IR.ShiftLeft32 b c)",
			"Filter": "!IsConstant(b) && IsUnsignedBetween32(c,1,4)",
			"Result": "(X86.Lea32 a b c 0)",
			"Priority": "55",
			"Variations": "Yes",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "ToMov32Constant1",
			"Expression": "X86.Lea32 0 0 c d",
			"Filter": "",
			"Result": "(X86.Mov32 d)",
			"Variations": "No",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "ToMov32Constant2",
			"Expression": "X86.Lea32 a 0 c 0",
			"Filter": "",
			"Result": "(X86.Mov32 a)",
			"Variations": "No",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "RemoveScale",
			"Expression": "X86.Lea32 a 0 c d",
			"Filter": "!IsOne(c)",
			"Result": "(X86.Lea32 a 0 1 d)",
			"Variations": "No",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Mov32ToLea32",
			"SubName": "By3Or5Or9",
			"Expression": "X86.Mul32 a b",
			"Filter": "!IsResult1Used(@) && !IsZero(a) && Contains(b, 3, 5, 9) && !AreAnyStatusFlagsUsed(@)",
			"Result": "(X86.Lea32 a a [Sub32(To32(b),1)] 0)",
			"Variations": "No",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "Shift32",
			"Expression": "X86.Lea32 a (X86.Shl32 b c) 1 d",
			"Filter": "IsVirtualRegister(b) && IsUnsignedBetween32(c, 1, 3) ",
			"Result": "(X86.Lea32 a b [ShiftLeft32(1,To32(c))] d)",
			"Variations": "No",
			"Log": "No"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "Shift32v2",
			"Expression": "X86.Lea32 (X86.Shl32 b c) a 1 d",
			"Filter": "IsVirtualRegister(a) && IsVirtualRegister(b) && IsUnsignedBetween32(c, 1, 3)",
			"Result": "(X86.Lea32 a b [ShiftLeft32(1,To32(c))] d)",
			"Variations": "No",
			"Log": "Yes"
		},
		{
			"Type": "Lea",
			"Name": "Lea32",
			"SubName": "Join",
			"Expression": "X86.Lea32 (X86.Lea32 a b c d1) 0 1 d2",
			"Filter": "IsResolvedConstant(d1) && IsResolvedConstant(d2)",
			"Result": "(X86.Lea32 a b c [Add32(To32(d1),To32(d2))])",
			"Variations": "No",
			"Log": "Yes"
		}
	]
}
