{
	"Family": "x64",
	"Section": "Optimizations.Auto",
	"Commutative": [
		"X64.Add32",
		"X64.And32",
		"X64.Or32",
		"X64.Add64",
		"X64.And64",
		"X64.Or64"
	],
	"Optimizations": [
		{
			"Type": "Standard",
			"Name": "Mov##",
			"SubName": "Coalescing",
			"Expression": "X64.Mov## (X64.Mov## a)",
			"Filter": "IsVirtualRegister(a)",
			"Result": "(X64.Mov## a)",
			"Variations": "No"
		},
		{
			"Type": "StrengthReduction",
			"Name": "Add##",
			"SubName": "ByZero",
			"Expression": "X64.Add## a 0",
			"Filter": "!AreStatusFlagUsed(@)",
			"Result": "(X64.Mov## a)",
			"Variations": "No"
		},
		{
			"Type": "StrengthReduction",
			"Name": "Sub##",
			"SubName": "ByZero",
			"Expression": "X64.Sub## a 0",
			"Filter": "!AreStatusFlagUsed(@)",
			"Result": "(X64.Mov## a)",
			"Variations": "Yes"
		},
		{
			"Type": "Shrink",
			"Name": "And##",
			"SubName": "ByFF",
			"Expression": "X64.And## a 0xFF",
			"Filter": "!IsCPURegister(a,%RSI) && !IsCPURegister(a,%RDI) && !AreStatusFlagUsed(@)",
			"Result": "(X64.Movzx8To## a)",
			"Variations": "Yes",
			"Log": "Yes",
			"Ignore": "Yes"
		},
		{
			"Type": "StrengthReduction",
			"Name": "Add##By1Not##",
			"SubName": "",
			"Expression": "X64.Add## (X64.Not## x) 1",
			"Filter": "IsVirtualRegister(x) && !IsCarryFlagUsed(@)",
			"Result": "(X64.Neg## x)",
			"Variations": "Yes"
		},
		{
			"Type": "StrengthReduction",
			"Name": "Inc##Not##",
			"SubName": "",
			"Expression": "X64.Inc## (X64.Not## x)",
			"Filter": "IsVirtualRegister(x) && !IsCarryFlagUsed(@)",
			"Result": "(X64.Neg## x)",
			"Variations": "No"
		},
		{
			"Type": "Specific",
			"Name": "And##Add##",
			"SubName": "ToBlsr##",
			"Expression": "X64.And## x (X64.Add## x -1)",
			"Filter": "IsVirtualRegister(x) && !IsCarryFlagUsed(@)",
			"Result": "(X64.Blsr## x)",
			"Variations": "Yes"
		},
		{
			"Type": "Simplication",
			"Name": "SubFromZero",
			"SubName": "",
			"Expression": "X64.Sub## 0 x",
			"Filter": "IsVirtualRegister(x)",
			"Result": "(X64.Neg## x)",
			"Variations": "No",
			"Log": "No"
		}
	]
}
